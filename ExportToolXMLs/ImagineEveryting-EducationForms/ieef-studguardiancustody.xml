  <field id="relCtjCntOid.ctjOID" header-source="constant" header-value="ContactExtId" /> 

  <!-- 
    ctjFieldA003 = "Legal Guardianship" 
    ctjFieldA001 = "ParentGuardian"

    LSSD uses ctjFieldA001 / "ParentGuardian", another district requested the additional field.
    -->
  <field id="ctjFieldA001" header-source="constant" header-value="IsCustodial" /> 
