<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    Status: TESTED
     [ ] Missing mailing address - its not currently possibly to get a mailing address in a single field
-->
<export table-id="tblSchool" header-row="true">

    <!-- Clevr Tenant ID -->   
    <!-- 
        EXPORT CUSTOMIZATION REQUIRED
        =============================
        Clevr uses this field to identify which customer an integration file belongs to.
        You will need to edit this value to match your Clevr tenant ID, or it will cause errors 
        when the export file is imported into Clevr.
    -->    
    <constant value="YOURTENANTIDHERE" header-source="constant" header-value="tenantId" />
    
    <field id="sklSchoolID" header-source="constant" header-value="proprietaryId" />
    <field id="sklSchoolID" header-source="constant" header-value="localId" />
    <field id="sklSchoolName" header-source="constant" header-value="schoolName" />
    <constant value="" header-source="constant" header-value="schoolStreetAddress" />
    <constant value="" header-source="constant" header-value="schoolPOBox" />
    <field id="relSklAdrOid.adrCity" header-source="constant" header-value="schoolCity" />
    <field id="relSklAdrOid.adrPostalCode" header-source="constant" header-value="schoolPostalCode" />
    <constant value="" header-source="constant" header-value="schoolMailingAddress" />   
   </export>