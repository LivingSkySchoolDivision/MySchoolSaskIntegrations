<export table-id="tblStdEnroll" header-row="true" value-delimiter=","> 

    <!--
        WARNING
        This will create a very large file, as it will include the entire provincial enrolment history for all of your students.
        In testing, this returned about 15 times as many records as we have students.
     -->

    <field id="relEnrStdOid.stdOID" header-source="constant" header-value="StudentOID" />
    <field id="relEnrStdOid.stdIDState" header-source="constant" header-value="StudentLearningID" />
    <field id="relEnrStdOid.stdIDLocal" header-source="constant" header-value="StudentLocalID" />       

    <field id="relEnrStdOid.relStdPsnOid.psnNameFirst" header-source="constant" header-value="FirstName" />  
    <field id="relEnrStdOid.relStdPsnOid.psnNameLast" header-source="constant" header-value="LastName" />
    <field id="relEnrStdOid.relStdPsnOid.psnDob" format="YYYY-MM-dd" header-source="constant" header-value="DateOfBirth" />

    <field id="relEnrSklOid.sklSchoolName" header-source="constant" header-value="SchoolName" />    
    <field id="relEnrSklOid.sklSchoolID" header-source="constant" header-value="SchoolDAN" />    
  
    <field id="enrEnrDate" header-source="constant" header-value="EnrolmentDate" />
    <field id="enrEnrCode" header-source="constant" header-value="EnrolmentCode" />
    <field id="enrEnrStatus" header-source="constant" header-value="EnrolmentStatus" />
    <field id="enrEnrType" header-source="constant" header-value="EnrolmentType" />    
    <field id="enrFieldA001" header-source="constant" header-value="GradeLevel" />
    <field id="enrYog" header-source="constant" header-value="YOG" />    

</export>